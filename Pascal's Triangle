# Print out Pascal's Triangle
# Used for loop,I think I need a little more time to use generator
def triangles(x):
	a=[1]
	print(a)
	for i in range(x):
		i=len(a)-1
		if i>0:
			while i>0:
				a[i]=a[i]+a[i-1]
				i-=1
			a+=[1]
			print(a)
		else:
			a+=[1]
			print(a)
triangles(int(input("input number: ")))
